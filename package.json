{
  "name": "paperplane-bugsnag",
  "version": "0.0.0",
  "description": "Bugsnag reporter for paperplane",
  "main": "index.js",
  "repository": "git@github.com:articulate/paperplane-bugsnag.git",
  "author": "articulate",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "test": "mocha --reporter=dot",
    "test:ci": "yarn lint && yarn test:coverage && yarn coverage",
    "test:coverage": "nyc yarn test"
  },
  "dependencies": {
    "paperplane": "2.x",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@articulate/spy": "^0.0.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "eslint": "^5.4.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  },
  "peerDependencies": {
    "bugsnag": "2.x"
  }
}
